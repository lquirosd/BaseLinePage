			<TextLine id="line_1486985595080_1596">
				<Coords points="1064,791 234,809 234,856 1064,838" />
				<TextEquiv>
					<Unicode>Man werde daher die jetzige Situation objektiv auffassen und</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985601593_1597">
				<Coords points="1053,839 240,857 240,903 1053,885" />
				<TextEquiv>
					<Unicode>Persönlichkeiten, namentlich eine Kritik des Verfahrens der Bundes-</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985609113_1598">
				<Coords points="1048,893 240,904 240,951 1048,940" />
				<TextEquiv>
					<Unicode>versammlung, vermeiden. An der Spitze des Unternehmens</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985614119_1599">
				<Coords points="1040,934 225,952 225,1006 1040,988" />
				<TextEquiv>
					<Unicode>steht unser Herr Stadtpräsident Sulzer, unterstützt von seinem</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985619996_1600">
				<Coords points="1044,990 232,1007 232,1059 1044,1042" />
				<TextEquiv>
					<Unicode>bekannten Anhange. Daß die Versammlung von andern, nicht</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985624181_1601">
				<Coords points="1063,1037 229,1060 229,1110 1063,1087" />
				<TextEquiv>
					<Unicode>zu diesem Anhange gehörenden Leuten noch zahlreich werde be-</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985631600_1602">
				<Coords points="1051,1096 227,1111 227,1160 1051,1145" />
				<TextEquiv>
					<Unicode>sucht werden, möchte ich bezweifeln. Manche, welche vielleicht</Unicode>
				</TextEquiv>
			</TextLine>
			<TextLine id="line_1486985648891_1603">
				<Coords points="1055,1141 222,1161 222,1212 1055,1192" />
				<TextEquiv>
					<Unicode>Theil nähmen, kennen den Geist der da dominiren wird; sie</Unicode>
